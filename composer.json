{
    "name": "liftov/craft-stage-file-proxy",
    "description": "Stage File Proxy is a general solution for getting production files on a development server on demand.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "stage file proxy"
    ],
    "support": {
        "docs": "https://github.com/Liftov/craft-stage-file-proxy/blob/master/README.md",
        "issues": "https://github.com/Liftov/craft-stage-file-proxy/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Wouter Van Scharen",
            "homepage": "http://liftov.be"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0",
        "php": "^8.0.2|^9.0"
    },
    "autoload": {
        "psr-4": {
          "liftov\\stagefileproxy\\": "src/"
        }
    },
    "extra": {
        "name": "Stage File Proxy",
        "handle": "stage-file-proxy",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/Liftov/craft-stage-file-proxy/CHANGELOG.md",
        "class": "liftov\\stagefileproxy\\StageFileProxy"
    }
}
